{
  "alias": "challenge-board-server",
  "version": 2,
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "app.js",
      "use": "@now/node-server",
      "config": { "bundle": false }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://challengeboard.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      },
      "dest": "app.js",
      "continue": true
    }
  ]
}
